{
  "version": 2,
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "backend.settings",
    "VERCEL": "1"
  },
  "functions": {
    "backend/wsgi.py": {
      "maxDuration": 30
    }
  },
  "buildCommand": "python build.py"
}